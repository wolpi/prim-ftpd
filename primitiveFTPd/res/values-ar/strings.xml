    <string name="prefTitlePubKeyAuth">المصادقة باستخدام مفتاح عام</string>
    <string name="prefSummaryPubKeyAuth">عند التفعيل، ضع مفتاح ssh2 العام الخاص بك في %s.\nSFTP فقط.</string>
